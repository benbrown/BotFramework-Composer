{
  "name": "composer-adaptive-card-designer",
  "version": "1.0.0",
  "description": "The Microsoft Adaptive Cards design as a customized Composer extension.",
  "main": "dist/extension.js",
  "scripts": {
    "build": "node ../build.js",
    "watch": "yarn build --watch"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "composer": {
    "enabled": true,
    "bundles": [
      {
        "id": "adaptive-cards-designer",
        "path": "./dist/Library.js"
      }
    ],
    "contributes": {
      "views": {
        "pages": [
          {
            "bundleId": "adaptive-cards-designer",
            "label": "Adaptive Cards",
            "icon": "Library"
          }
        ]
      }
    }
  },
  "resolutions": {
    "@botframework-composer/types": "file:../../Composer/packages/types"
  },
  "dependencies": {
    "@bfc/extension": "file:../../Composer/packages/extension",
    "@bfc/extension-client": "file:../../Composer/packages/extension-client",
    "@bfc/ui-shared": "file:../../Composer/packages/lib/ui-shared",
    "@emotion/core": "^10.0.35",
    "@uifabric/fluent-theme": "^7.1.4",
    "adaptive-expressions": "^4.11.1",
    "adaptivecards": "^2.7.0",
    "adaptivecards-designer": "^2.0.0",
    "adaptivecards-templating": "^2.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.2",
    "file-loader": "^6.2.0",
    "format-message": "^6.2.3",
    "markdown-it": "^12.0.4",
    "monaco-editor": "^0.22.3",
    "monaco-editor-webpack-plugin": "^3.0.0",
    "office-ui-fabric-react": "7.71.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "style-loader": "^2.0.0"
  },
  "devDependencies": {
    "@botframework-composer/types": "file:../../Composer/packages/types",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.6.2",
    "@types/passport": "^1.0.3",
    "@types/react": "^16.9.53",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
