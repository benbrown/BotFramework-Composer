{
  "name": "asset-library",
  "version": "1.0.0",
  "description": "Manage external dialog assets.",
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn clean && yarn build:client && yarn build:node",
    "build:client": "webpack --config webpack.config.js",
    "build:node": "tsc --project tsconfig.node.json",
    "clean": "rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "composer": {
    "enabled": true,
    "bundles": [
      {
        "id": "asset-library",
        "path": "./lib/pages/asset-library.js"
      }
    ],
    "contributes": {
      "views": {
        "pages": [
          {
            "bundleId": "asset-library",
            "label": "Asset Library",
            "icon": "Library"
          }
        ]
      }
    }
  },
  "dependencies": {
    "@bfc/extension-client": "file:../../Composer/packages/extension-client",
    "@bfc/ui-shared": "file:../../Composer/packages/lib/ui-shared",
    "@emotion/core": "^10.0.35",
    "@uifabric/fluent-theme": "^7.1.4",
    "date-fns": "^2.16.1",
    "format-message": "^6.2.3",
    "office-ui-fabric-react": "7.71.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@botframework-composer/types": "file:../../Composer/packages/types",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.53",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.0",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
