{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Task": {
            "type": "string",
            "$entities": [
                "utterance"
            ]
        },
        "ToDoType": {
            "type": "string",
            "enum": [
                "grocery",
                "shopping",
                "sports"
            ]
      
        }
       
    },
    "required": [
        "Task",
        "ToDoType"
    ],
    "$requires" : [
        "standard.schema"
    ]
}