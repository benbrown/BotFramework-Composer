{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"properties": {
		"Quantity": {
			"type": "number",
			"minimum": 1,
			"maximum": 10,
			"$entities": [
				"number:Quantity",
				"number"
			]
		},
		"Length": {
			"type": "object",
			"$entities": [
				"dimension:Length",
				"dimension"
			],
			"required": [
				"number",
				"units"
			],
			"properties": {
				"number": {
					"type": "number"
				},
				"units": {
					"type": "string"
				}
			}
		},
		"Name": {
			"type": "string",
			"$entities": [
				"personName",
				"utterance"
			]
		},
		"Bread": {
			"type": "string",
			"enum": [
				"multiGrainWheat",
				"rye",
				"white",
				"wholeWheat"
			],
			"$entities": [
				"BreadEntity"
			]
		},
		"Meat": {
			"type": "string",
			"enum": [
				"bacon",
				"chicken",
				"ham",
				"pulled pork",
				"roast beef",
				"salami",
				"turkey",
				"none"
			],
			"$entities": [
				"MeatEntity"
			]
		},
		"Cheese": {
			"type": "string",
			"enum": [
				"american",
				"cheddar",
				"feta",
				"gouda",
				"pepper jack",
				"provolone",
				"swiss",
				"none"
			],
			"$entities": [
				"CheeseEntity"
			]
		},
		"Toppings": {
			"type": "array",
			"items": {
				"type": "string",
				"enum": [
					"tomato",
					"lettuce",
					"pickles",
					"greenPeppers",
					"redPepppers",
					"whiteOnions",
					"redOnions"
				],
				"maxItems": 3
			},
			"$entities": [
				"ToppingsEntity"
			]
		},
		"Sauces": {
			"type": "array",
			"items": {
				"type": "string",
				"enum": [
					"pepper",
					"salt",
					"yellowMustard",
					"dijonMustard",
					"mayo",
					"vinegar"
				]
			},
			"$entities": [
				"SaucesEntity"
			]
		},
		"Price": {
			"type": "object",
			"$entities": [
				"money:Price",
				"money"
			],
			"required": [
				"number",
				"units"
			],
			"properties": {
				"number": {
					"type": "number"
				},
				"units": {
					"type": "string"
				}
			}
		},
		"CancelConfirmation": {
			"type": "boolean",
			"$entities": [
				"boolean"
			]
		},
		"ChangePropertyConfirmation": {
			"type": "boolean",
			"$entities": [
				"boolean"
			]
		},
		"CompleteConfirmation": {
			"type": "string",
			"$entities": [
				"boolean",
				"PROPERTYName"
			]
		},
		"PropertyToChange": {
			"type": "string",
			"enum": [
				"Quantity",
				"Length",
				"Name",
				"Bread",
				"Meat",
				"Cheese",
				"Toppings",
				"Sauces",
				"Price"
			],
			"$entities": [
				"PROPERTYName"
			]
		},
		"PropertyToRemember": {
			"type": "string",
			"enum": [
				"Quantity",
				"Length",
				"Name",
				"Bread",
				"Meat",
				"Cheese",
				"Toppings",
				"Sauces",
				"Price"
			],
			"$entities": [
				"PROPERTYName"
			]
		}
	},
	"required": [
		"Name",
		"Bread",
		"Cheese",
		"Meat",
		"Toppings",
		"Sauces",
		"Price"
	],
	"$expectedOnly": [
		"boolean",
		"PROPERTYName",
		"utterance",
		"keyPhrase",
		"personName"
	],
	"$operations": [
		"AddToProperty",
		"RemoveFromProperty",
		"ClearProperty",
		"ShowProperty"
	],
	"$public": [
		"Quantity",
		"Length",
		"Name",
		"Bread",
		"Meat",
		"Cheese",
		"Toppings",
		"Sauces",
		"Price"
	],
	"definitions": {
		"PROPERTYName": {
			"type": "string",
			"enum": [
				"Quantity",
				"Length",
				"Name",
				"Bread",
				"Meat",
				"Cheese",
				"Toppings",
				"Sauces",
				"Price"
			],
			"$entities": [
				"PROPERTYName"
			]
		}
	},
	"$defaultOperation": "AddToProperty"
}