{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"properties": {
		"Task": {
			"type": "string",
			"$entities": [
				"utterance"
			]
		},
		"ToDoType": {
			"type": "string",
			"enum": [
				"grocery",
				"shopping",
				"sports"
			],
			"$entities": [
				"ToDoTypeEntity"
			]
		},
		"CancelConfirmation": {
			"type": "boolean",
			"$entities": [
				"boolean"
			]
		},
		"ChangePropertyConfirmation": {
			"type": "boolean",
			"$entities": [
				"boolean"
			]
		},
		"CompleteConfirmation": {
			"type": "string",
			"$entities": [
				"boolean",
				"PROPERTYName"
			]
		},
		"PropertyToChange": {
			"type": "string",
			"enum": [
				"Task",
				"ToDoType"
			],
			"$entities": [
				"PROPERTYName"
			]
		},
		"PropertyToRemember": {
			"type": "string",
			"enum": [
				"Task",
				"ToDoType"
			],
			"$entities": [
				"PROPERTYName"
			]
		}
	},
	"required": [
		"Task",
		"ToDoType"
	],
	"$expectedOnly": [
		"boolean",
		"PROPERTYName",
		"utterance",
		"keyPhrase",
		"personName"
	],
	"$operations": [
		"AddToProperty",
		"RemoveFromProperty",
		"ClearProperty",
		"ShowProperty"
	],
	"$public": [
		"Task",
		"ToDoType"
	],
	"definitions": {
		"PROPERTYName": {
			"type": "string",
			"enum": [
				"Task",
				"ToDoType"
			],
			"$entities": [
				"PROPERTYName"
			]
		}
	},
	"$defaultOperation": "AddToProperty"
}