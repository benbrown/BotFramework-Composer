{
  "name": "@bfc/ui-plugin-teams",
  "version": "0.0.0",
  "private": true,
  "description": "Botframework Composer plugin that adds custom form UI for Teams.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "tsc --watch --preserveWatchOutput",
    "build": "tsc --build ./tsconfig.build.json",
    "lint": "eslint --quiet src",
    "prepare": "yarn build",
    "extract": "cross-env NODE_ENV=production format-message extract -g underscored_crc32 -o locales/en-US.json src/**/*.ts src/**/*.tsx"
  },
  "keywords": [
    "botframework",
    "composer",
    "Microsoft Teams"
  ],
  "author": "zeye@microsoft.com",
  "license": "MIT",
  "peerDependencies": {
    "@bfc/extension-client": "*",
    "format-message": "^6.2.3"
  },
  "devDependencies": {
    "@bfc/extension-client": "*",
    "format-message": "^6.2.3",
    "format-message-cli": "^6.2.3"
  },
  "dependencies": {
    "@bfc/shared": "*",
    "@bfc/ui-shared": "*"
  }
}
